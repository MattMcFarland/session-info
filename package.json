{
  "name": "session-info",
  "version": "0.0.3",
  "description": "Express middleware session diagnostic that console.logs the current session info",
  "main": "lib/index.js",
  "scripts": {
    "prepublish": "npm test && npm run build",
    "test": "npm run lint && npm run check && mocha $npm_package_options_mocha",
    "lint": "eslint src",
    "testonly": "npm run lint && npm test",
    "check": "flow check",
    "build": "rm -rf lib/* && babel src --ignore __tests__ --out-dir lib",
    "watch": "babel tasks/watch.js | node",
    "cover": "babel-node node_modules/.bin/isparta cover --root src --report html node_modules/.bin/_mocha -- $npm_package_options_mocha",
    "cover:lcov": "babel-node node_modules/.bin/isparta cover --root src --report lcovonly node_modules/.bin/_mocha -- $npm_package_options_mocha"
  },
  "options": {
    "mocha": "--require tasks/mocha-bootload src/**/__tests__/**/*.js"
  },
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "lib"
  ],
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.6",
    "babel-plugin-transform-flow-strip-types": "^6.3.15",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "^1.14.2",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.2.0",
    "compression": "^1.6.0",
    "cookie-parser": "^1.3.5",
    "coveralls": "^2.11.6",
    "eslint": "^1.10.3",
    "express": "~4.13.1",
    "express-session": "^1.12.1",
    "figlet": "^1.1.1",
    "flow-bin": "^0.20.1",
    "isparta": "^4.0.0",
    "mocha": "^2.3.4",
    "sane": "^1.3.0",
    "serve-favicon": "~2.3.0",
    "supertest": "^1.1.0"
  },
  "peerDependencies": {
    "express-session": "^1.12.1"
  },
  "keywords": [
    "express",
    "middleware",
    "connect",
    "diagnostic",
    "log",
    "express-session",
    "session"
  ],
  "author": "Matt McFarland",
  "license": "MIT"
}
